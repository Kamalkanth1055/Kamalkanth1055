#include <stdio.h>
int main() 
{
   int a,b,c;
   printf("\nenter the A value");
   scanf("%d",&a);
   printf("\nenter the B value");
   scanf("%d",&b);
   printf("\nenter the C value");
   scanf("%d",&c);
   if(a>b)
   printf("A is bigger");
  else if(b>c)
   printf("B is bigger");
   else
   printf("C is bigger");
}
